{
  "$schema": "https://schema.tauri.app/config/2.0.0-rc",
  "productName": "Google Messages",
  "version": "0.1.0",
  "identifier": "com.gridironsolutions.google-messages-for-macos",
  "build": {
    "devUrl": "https://messages.google.com/web"
  },
  "app": {
    "withGlobalTauri": true,
    "windows": [
      {
        "title": "Google Messages",
        "width": 1024,
        "height": 768,
        "resizable": true,
        "fullscreen": false
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "icon": [
      "../icons/32x32.png",
      "../icons/128x128.png",
      "../icons/256x256.png",
      "../icons/512x512.png",
      "../icons/icon.icns"
    ],
    "macOS": {
      "minimumSystemVersion": "10.13"
    }
  }
}
